{
	"name": "crossforge",
	"version":  "0.1",
	"dependencies": [
		"eigen3",
		"glad",
		"glfw3",
		"assimp",
		"freetype",
		"libigl",
		"libwebp",
		"tinyxml2",
		"tinygltf",
		{
			"name": "imgui",
			"default-features": false,
			"features": [ "opengl3-binding", "glfw-binding" ]
		},
		{
			"name": "opencv",
			"default-features": false,
			"features": [ "ffmpeg" ]
		},
		"openxr-loader"
	]
}

