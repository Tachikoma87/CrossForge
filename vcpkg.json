{
  "name": "crossforge",
  "version": "0.1",
  "dependencies": [
    "eigen3",
    "glad",
    "glfw3",
    "assimp",
    "freetype",
    "libigl",
    "libwebp",
    "tinyxml2",
    "tinygltf",
    "libjpeg-turbo",
    "tinyfiledialogs",
    {
      "name": "ffmpeg",
      "default-features": true,
      "features": [
        "x264"
      ]
    },
    {
      "name": "imgui",
      "default-features": false,
      "features": [
        "opengl3-binding",
        "glfw-binding"
      ]
    }
  ]
}
